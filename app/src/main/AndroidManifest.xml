<!--    android:name="cn.rongcloud.quickstart.MyApp"-->
    <application
        android:name="cn.rongcloud.quickstart.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/playstore"
        android:usesCleartextTraffic="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"

        android:supportsRtl="true">
        <meta-data
            android:name="design_width"
            android:value="360"/>
        <meta-data
            android:name="design_height"
            android:value="360"/>
        <activity
            android:name="cn.rongcloud.quickstart.indexpage.SplashActivity"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name="cn.rongcloud.quickstart.loginpage.LoginActivity"/>
        <activity android:name="cn.rongcloud.quickstart.forgetpage.forgetActivity"/>
        <activity android:name="cn.rongcloud.quickstart.forgetpage.forgetEmailActivity"/>
        <activity android:name="cn.rongcloud.quickstart.registerpage.registerEmailActivity"/>
        <activity android:name="cn.rongcloud.quickstart.registerpage.registerActivity"/>
        <activity android:name="cn.rongcloud.quickstart.userpage.userListActivity"/>
        <activity android:name="cn.rongcloud.quickstart.userpage.userSettingActivity"/>
        <activity android:name="cn.rongcloud.quickstart.homepage.homeListActivity"/>
        <activity android:name="cn.rongcloud.quickstart.homepage.createMeetingActivity"/>
        <activity android:name="cn.rongcloud.quickstart.homepage.detailsMeetingActivity"/>
        <activity android:name="cn.rongcloud.quickstart.homepage.joinMeetingActivity"/>
        <activity android:name="cn.rongcloud.quickstart.MeetingRoom.MeetingActivity"/>
        <activity android:name="cn.rongcloud.quickstart.userpage.userAlterActivity"/>




    </application>

</manifest>
